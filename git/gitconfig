[core]
	excludesfile = /Users/wellis/.gitignore
	ui = on
[alias]
	co = checkout
	st = status
	dc = diff --cached
	d = diff
	cm = checkout master
	p = pull
	cb = checkout -b
	wip = !git add --all && git commit -m \"WIP\" --no-verify
	b = branch
	undo = reset HEAD~
	rh = reset --hard HEAD
	oops = !git add --all && git commit --amend --reuse-message=HEAD
	c = commit -m
	aa = add --all
    fp = push --force-with-lease
[rebase]
	autosquash = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[merge]
	conflictstyle = diff3
[pull]
	ff = only
